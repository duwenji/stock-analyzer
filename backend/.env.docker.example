# Database configuration
DB_NAME=stock_analyzer
DB_USER=stock_user
DB_PASSWORD=strong_password

# AI Agent configuration
AGENT_TYPE=mcpagent
MAX_WORKERS=2
REQUEST_INTERVAL=0.5
MAX_RETRIES=3

# API Keys (実際の値を設定してください)
DEEPSEEK_API_KEY=your_api_key_here

# Moving average settings for chart
SHORT_MA_WINDOW=12
LONG_MA_WINDOW=26

# ゴールデンクロス/デッドクロス計算用
GOLDEN_DEAD_SHORT_WINDOW=12
GOLDEN_DEAD_LONG_WINDOW=26

# RSI計算用
RSI_WINDOW=12

# MACD計算用
MACD_FAST=12
MACD_SLOW=26
MACD_SIGNAL=9
